{
  "name": "mind-for-code",
  "private": true,
  "description": "A blog discussing various Web Developer Topics",
  "version": "0.1.0",
  "author": "Sam Overton <samovertonjr@gmail.com>",
  "bugs": {
    "url": "https://github.com/samovertonjr/mindforcode/issues"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
